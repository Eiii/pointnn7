{
  "experiment_path": "./output/sc2dist",
  "problem_type": "StarcraftScene",
  "problem_args": {
    "data_path": "data/sc2scene",
    "num_pred": 4,
    "max_hist": 5,
    "hist_dist": "tail",
    "hist_dist_args": {"max": 10},
    "pred_dist": "uniform",
    "pred_dist_args": {"max": 10}
  },
  "epochs": 0.1,
  "entries": [
    {
      "name": "Test",
      "net": "SC2TPCSingle",
      "train_args": {
        "batch_size": 3,
        "lr": 0.003,
        "sched": "none",
        "report_every": 0.01,
        "valid_every": 0.05,
        "period": 10
      },
      "net_args": {
        "neighborhood_sizes": [16, 32, 32],
        "latent_sizes": [32, 64, 64],
        "target_size": 64,
        "combine_hidden": [64, 64],
        "weight_hidden": [32, 32],
        "c_mid": 32,
        "final_hidden": [64, 64],
        "decode_hidden": [64, 64, 64],
        "neighbors": 8,
        "timesteps": 4,
        "time_factor": 2.2
      },
      "repeat": 1
    }
  ]
}
  
