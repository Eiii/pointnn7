{
  "experiment_path": "./output/weather",
  "problem_type": "Weather",
  "problem_args": {
    "data_path": "data/weather",
    "HACK_SNIP": 50
  },
  "epochs": 6,
  "entries": [
    {
      "name": "Test",
      "net": "WeatherTPC",
      "train_args": {
        "batch_size": 2,
        "report_every": 0.01,
        "valid_every": 0.1,
        "lr": 0.00001,
        "period": 2,
        "optim": "sgd",
        "disable_valid": true
      },
      "net_args": {
        "neighborhood_sizes": [16, 32, 32],
        "latent_sizes": [32, 64, 64],
        "combine_hidden": [64, 64],
        "neighbors": 8,
        "timesteps": 9,
        "station_dropout": 0.2
      },
      "repeat": 1
    }
  ]
}
  
